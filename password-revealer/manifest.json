{
    "manifest_version": 2,
    "content_scripts": [{
        "exclude_globs":    [],
        "include_globs":    ["*"],
        "js":               ["password-revealer.user.js"],
        "matches":          ["http://*/*", "file:///*", "https://*/*"],
        "run_at": "document_end"
    }],
    "converted_from_user_script": true,
    "description":  "Show password entered in password inputs",
    "name":         "Password Revealer",
    "version":      "1",
    "icons": {
        "128": "favicon.png"
    }
}
